{% extends 'base.html.twig' %}

{% block title %}Liste des projets{% endblock %}

{% block body %}

    <section class="container">
        <h1>Liste des projets</h1>
    </section>

    <!-- Afficher uniquement la liste des projets -->
    <section class="project">
        <div class="container">
            <div class="row">
                <div class="col-4">
                    {% for project in projects %}
                        {% include 'project/_thumbnail.html.twig' with {'project':project} %}
                    {% endfor %}
                </div>


            </div>
    </section>


{% endblock %}
